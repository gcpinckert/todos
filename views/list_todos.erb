<section id="todos">
  <header>
    <h2><%= @list[:name] %></h2>
    <ul>
      <li>
        <a href="/lists/<%= params[:number] %>/edit" class="edit">Edit List</a>
      </li>
    </ul>
  </header>

  <ul>
    <% @list[:todos].each do |todo| %>
      <li><%= todo %></li>
    <% end %>
  </ul>
</section>

<% content_for :header_links do%>
  <a href="/lists">All Lists</a>
<% end %>